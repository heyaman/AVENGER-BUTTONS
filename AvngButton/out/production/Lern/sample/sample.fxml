<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button fx:id="captain" text="Captain america" onAction="#Buton" textFill="aqua" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button fx:id="hulk" text="Hulk" onAction="#Buton" textFill="chartreuse" GridPane.rowIndex="0"
            GridPane.columnIndex="1"/>
    <Button fx:id="Ironman" text="IRON MAN" onAction="#Buton" textFill="darkred" GridPane.rowIndex="1"
            GridPane.columnIndex="0"/>
    <Button fx:id="Antman" text="Ant man" onAction="#Buton" textFill="burlywood" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    <TextField fx:id="buton" GridPane.rowIndex="2" GridPane.columnIndex="0" />








</GridPane>