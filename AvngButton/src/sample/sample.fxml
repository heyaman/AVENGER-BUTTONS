<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="sample.Controller">
    <Button fx:id="captain" onAction="#Buton" text="Captain america" textFill="aqua" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Button fx:id="hulk" onAction="#Buton" text="Hulk" textFill="chartreuse" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    <Button fx:id="Ironman" onAction="#Buton" text="IRON MAN" textFill="darkred" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button fx:id="Antman" onAction="#Buton" text="Ant man" textFill="burlywood" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextField fx:id="buton" GridPane.columnIndex="0" GridPane.rowIndex="2" />
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>








</GridPane>
